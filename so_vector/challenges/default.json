{
  "name": "index-and-search",
  "description": "Indexes vectors, then executes searches under various conditions",
  "default": true,
  "schedule": [
    {
      "tags": ["setup"],
      "operation": {
        "operation-type": "delete-index"
      }
    },
    {
      "tags": ["setup"],
      "operation": {
        "operation-type": "create-index"
      }
    },
    {
      "tags": ["health"],
      "operation": {
        "operation-type": "cluster-health",
        "request-params": {
          "wait_for_status": "green"
        },
        "retry-until-success": true
      }
    },
    {
      "name": "index-append",
      "tags": ["index"],
      "operation": {
        "operation-type": "bulk",
        "bulk-size": {{bulk_size | default(500)}},
        "ingest-percentage": {{ingest_percentage | default(100)}},
        "corpora": {{corpora | default("so_vector_float") | tojson }}
    },
      "warmup-time-period": {{ bulk_warmup | default(40) | int }},
      "clients": {{bulk_indexing_clients | default(1)}}
    },
    {
      "name": "refresh-after-index",
      "tags": ["index"],
      "operation": {
        "operation-type": "refresh",
        "request-timeout": 1000,
        "include-in-reporting": true
      }
    },
    {
      "name": "wait-until-merges-finish",
      "operation": {
        "operation-type": "index-stats",
        "index": "_all",
        "condition": {
        "path": "_all.total.merges.current",
        "expected-value": 0
      },
      "retry-until-success": true,
      "include-in-reporting": false
      },
      "tags": ["index"]
    },
    {
      "name": "knn-search-10-50-acceptedAnswerId",
      "tags": ["search"],
      "operation": "knn-search-10-50-acceptedAnswerId",
      "warmup-iterations": {{ warmup_iterations | default(100) }},
      "iterations": {{ iterations | default(100) }},
      "clients": 1
    },
    {
      "name": "knn-recall-10-50-acceptedAnswerId",
      "tags": ["search"],
      "operation": "knn-recall-10-50-acceptedAnswerId",
      "warmup-iterations": 0,
      "iterations": 1,
      "clients": 1
    },
